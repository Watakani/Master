from data.data import Dataset

import torch
import numpy as np

DATAPATH = '../data/power/data.npy'

class Power(Dataset):
    def __init__(self):
        super().__init__()

        data = np.load(DATAPATH)
        rng = np.random.RandomState(42)
        
        rng.shuffle(data)
        print(data.shape)
        data = np.delete(data, 3, axis=1)
        data = np.delete(data, 1, axis=1)

        self.n, self.dim_input = data.shape

        volage_noise = 0.01*rng.rand(self.n, 1)
        print(data.shape)


